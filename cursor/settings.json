{
    "apc.statusBar": {
        "position": "editor-bottom"
    },
    "apc.font.family": "JetBrains Mono",
    "window.menuBarVisibility": "toggle",
    "workbench.activityBar.location": "bottom",
    "window.commandCenter": false,
    "window.title": "Patil",
    "vscode-neovim.compositeKeys": {
        "jj": {
            "command": "vscode-neovim.escape"
        }
    },
    "vscode-neovim.neovimExecutablePaths.win32": "C:\\Program Files\\Neovim\\bin\\nvim.exe",
    "html.autoclosingtags": true,
    "javascript.autoClosingTags": true,
    "typescript.autoClosingTags": true,
    "breadcrumbs.enabled": true,
    "reactSnippets.settings.importReactOnTop": false,
    "editor.cursorBlinking": "phase",
    "editor.find.cursorMoveOnType": false,
    "editor.cursorSmoothCaretAnimation": "on",
    "git.openRepositoryInParentFolders": "never",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.accessibilitySupport": "off",
    "git.autofetch": true,
    "editor.lineHeight": 1.2,
    "workbench.startupEditor": "none",
    "security.workspace.trust.untrustedFiles": "open",
    "editor.mouseWheelZoom": true,
    "editor.fontSize": 16,
    "editor.fontLigatures": true,
    "workbench.colorCustomizations": {
        "statusBar.background": "#8FBCBB",
        "statusBar.noFolderBackground": "#8FBCBB",
        "statusBar.debuggingBackground": "#026362",
        "statusBar.foreground": "#000",
        "statusBar.debuggingForeground": "#000"
    },
    "editor.lineNumbers": "relative",
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": true,
    "editor.linkedEditing": true,
    "editor.suggest.insertMode": "replace",
    "workbench.tips.enabled": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "typescript.preferences.importModuleSpecifier": "relative",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "extensions.autoUpdate": "onlyEnabledExtensions",
    "extensions.ignoreRecommendations": true,
    "javascript.suggest.autoImports": true,
    "typescript.suggest.autoImports": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.sortImports": "explicit",
        "source.addMissingImports": "explicit"
    },
    "editor.fontFamily": "JetBrainsMono Nerd Font",
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.minimap.enabled": false,
    "editor.folding": false,
    "symbols.hidesExplorerArrows": true,
    "editor.formatOnSave": true,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "update.mode": "manual",
    "git.confirmSync": false,
    "liveServer.settings.donotVerifyTags": true,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 4,
    "workbench.editor.limit.perEditorGroup": true,
    "github.copilot.editor.enableAutoCompletions": true,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "github.copilot.preferredAccount": "the-vaibhav",
    "workbench.sideBar.location": "right",
    "workbench.iconTheme": "symbols",
    "terminal.integrated.fontFamily": "JetBRainsMono Nerd Font",
    "vscode-neovim.neovimClean": false,
    "workbench.colorTheme": "Catppuccin Frapp√©",
    "workbench.editor.showTabs": "none",
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "cursor.cpp.disabledLanguages": [
        "plaintext",
        "markdown",
        "scminput"
    ],
    "workbench.activityBar.orientation": "vertical"
}